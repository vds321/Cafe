<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Cafe.ViewModels.MainViewModels"
                    xmlns:view="clr-namespace:Cafe.Views">

    <DataTemplate DataType="{x:Type vm:MenuViewModel}">
        <view:MenuView DataContext="{Binding}"/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ManageCafeViewModel}">
        <view:ManageCafeView DataContext="{Binding}"/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:WarehouseViewModel}">
        <view:WarehouseView DataContext="{Binding}"/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:OrderViewModel}">
        <view:OrderView DataContext="{Binding}"/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ReportViewModel}">
        <view:ReportView DataContext="{Binding}"/>
    </DataTemplate>

</ResourceDictionary>