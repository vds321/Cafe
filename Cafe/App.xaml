<Application x:Class="Cafe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:l="clr-namespace:Cafe.Views"
             xmlns:vm="clr-namespace:Cafe.Service.Registrator"
             StartupUri="Views/Windows/MainWindowView.xaml"
             >
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/CommandDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Brushes/BrushesDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Style/ButtonStyle.xaml"/>
                <ResourceDictionary Source="Resources/Template/ModelMainView.xaml"/>
                <ResourceDictionary Source="Resources/ConverterDictionary/ConverterDictionary.xaml"/>
                <ResourceDictionary Source="Resources/Style/TotalPanelStyle.xaml"/>
                <ResourceDictionary Source="Resources/Style/GridViewStyle.xaml"/>
                <ResourceDictionary Source="Resources/Style/StackPanelTemplate.xaml"/>
                <ResourceDictionary Source="Resources/Template/ButtonControl.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <vm:ViewModelLocator x:Key="Locator"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
